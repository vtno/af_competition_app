  <thead class="thead-inverse <%= "#{score_type}_#{range}" %>">
    <tr>
      <th><%= range %>&nbsp;เมตร</th>
      <th>ประเภทการแข่งขัน</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody class='<%= "#{score_type}_#{range}_body" %>'>
    <% if range == 18 %>
      <%=  render partial: 'competition_type', locals: { range: range, score_type: score_type, number: '1', compet_level: 'U12', bow_type: 'Recurve' }%>
      <%=  render partial: 'competition_type', locals: { range: range, score_type: score_type, number: '2', compet_level: 'Open 18', bow_type: 'Recurve' }%>
      <%=  render partial: 'competition_type', locals: { range: range, score_type: score_type, number: '3', compet_level: 'Open 18', bow_type: 'Barebow' }%>
      <%=  render partial: 'competition_type', locals: { range: range, score_type: score_type, number: '4', compet_level: 'Open 18', bow_type: 'Compound' }%>
    <% elsif range == 10 %>
      <%=  render partial: 'competition_type', locals: { range: range, score_type: score_type, number: '1', compet_level: 'U12', bow_type: 'Recurve' }%>
    <% end %>
  </tbody>

