<tr class="row-<%= count %>" data-id="<%= @score.id %>" data-round="<%= count %>" data-compet="<%= @competition.id%>">
  <th>ชุดที่ <%= count %></th>
  <td class="score" data-position="0">
    <span><%= render_score score[0]%></span>
  </td>
  <td class="score" data-position="1">
    <span><%= render_score score[1]%></span>
  </td>
  <td class="score" data-position="2">
     <span><%=render_score score[2]%></span>
  </td>
  <% if @competition.games.first.team %>
    <td class="score" data-position="3">
      <span><%= render_score score[3]%></span>
    </td>
    <td class="score" data-position="4">
      <span><%= render_score score[4]%></span>
    </td>
    <td class="score point" id="point" data-position="<%= count - 1 %>">
      <span><%= @score.points[count - 1] %></span>
    </td>
  <% end %>
  <td class="score total-score">
    <span><%= Score.calc(score)%></span>
  </td>
  <% if @competition.ten_set? %>
    <td class=" score count-10">
      <span><%= Score.count_score(score, 10)%></span>
    </td>
    <td class=" score count-9">
      <span><%= Score.count_score(score, 9)%></span>
    </td>
  <% else %>
    <td class=" score count-x-10">
      <span><%= Score.count_x_10(score)%></span>
    </td>
    <td class=" score count-x">
      <span><%= Score.count_score(score, 11)%></span>
    </td>
  <% end %>
</tr>
